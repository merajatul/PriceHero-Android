<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/booking"
    tools:context="com.lubnasweety.pricehero.Booking">
    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/right"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="110dp"
        android:layout_marginTop="126dp"
        android:layout_marginLeft="81dp"
        tools:ignore="ContentDescription,RtlHardcoded"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView14"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:text="Your Order has been successfuly sent......."
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:ignore="HardcodedText"
         />

    <Button
        android:id="@+id/okButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:text="OK"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/textView10"
        android:layout_alignEnd="@+id/imageView14"
        android:layout_marginTop="14dp" />
    <Button
        android:id="@+id/outButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:text="Logout"
        tools:ignore="HardcodedText"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/okButton"
        android:layout_alignStart="@+id/okButton" />


</RelativeLayout>
