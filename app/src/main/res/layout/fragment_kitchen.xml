<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@drawable/register"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textViewKitchen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kitchen Products"
        android:textStyle="bold"
        android:textSize="23dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.077"
        android:layout_marginTop="32dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageViewblender"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/blinder"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_below="@+id/textViewKitchen"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp" />

    <ImageView
        android:id="@+id/imageViewkitchen"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/kitchen"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_alignTop="@+id/imageViewblender"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageViewpan"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/pan"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_alignTop="@+id/imageViewkitchen"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="18dp" />

    <ImageView
        android:id="@+id/imageViewcooker"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/cooker"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_below="@+id/imageViewblender"
        android:layout_alignStart="@+id/imageViewblender"
        android:layout_marginTop="25dp" />


    <ImageView
        android:id="@+id/imageViewricecooker"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/ricecooker"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_alignTop="@+id/imageViewcooker"
        android:layout_alignStart="@+id/imageViewkitchen" />

    <ImageView
        android:id="@+id/imageViewboul"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/boul"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_alignTop="@+id/imageViewricecooker"
        android:layout_alignStart="@+id/imageViewpan" />

    <ImageView
        android:id="@+id/imageViewspoon"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/spoon"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_below="@+id/imageViewcooker"
        android:layout_alignStart="@+id/imageViewcooker"
        android:layout_marginTop="37dp" />

    <ImageView
        android:id="@+id/imageViewgas"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/gas"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_alignTop="@+id/imageViewspoon"
        android:layout_alignStart="@+id/imageViewricecooker" />

    <ImageView
        android:id="@+id/imageViewboull"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/boull"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_alignTop="@+id/imageViewgas"
        android:layout_alignStart="@+id/imageViewboul" />

</RelativeLayout>