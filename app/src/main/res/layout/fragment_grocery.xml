<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@drawable/register"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageViewOil"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/oil"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="80dp"
        android:layout_marginBottom="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:id="@+id/imageViewRice"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/rice"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.171"
        android:layout_alignTop="@+id/imageViewTea"
        android:layout_alignStart="@+id/imageViewFortunerice" />
    <ImageView
        android:id="@+id/imageViewTea"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/tea"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.171"
        app:layout_constraintHorizontal_bias="0.904"
        android:layout_alignTop="@+id/imageViewMilk"
        android:layout_alignStart="@+id/imageViewsureshOil" />
    <ImageView
        android:id="@+id/imageViewMilk"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/milk"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.479"
        android:layout_above="@+id/imageViewbiscuit"
        android:layout_alignStart="@+id/imageViewbiscuit"
        android:layout_marginBottom="31dp" />
    <ImageView
        android:id="@+id/imageViewFortunerice"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/fortunerice"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.479"
        android:layout_alignTop="@+id/imageViewsureshOil"
        android:layout_alignStart="@+id/imageViewata" />
    <ImageView
        android:id="@+id/imageViewsureshOil"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/sureshoil"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.904"
        app:layout_constraintVertical_bias="0.498"
        android:layout_alignTop="@+id/imageViewbiscuit"
        android:layout_alignStart="@+id/imageViewteaPack" />
    <ImageView
        android:id="@+id/imageViewbiscuit"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/biscuit"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.01"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        android:layout_above="@+id/imageViewOil"
        android:layout_alignParentStart="true"
        android:layout_marginStart="14dp"
        android:layout_marginBottom="53dp" />
    <ImageView
        android:id="@+id/imageViewata"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/ata"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        android:layout_alignTop="@+id/imageViewteaPack"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="11dp" />
    <ImageView
        android:id="@+id/imageViewteaPack"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/teapack"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.832"
        app:layout_constraintHorizontal_bias="0.943"
        android:layout_alignTop="@+id/imageViewOil"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textViewGrocery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Grocery Products"
        android:textStyle="bold"
        android:textSize="23dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.077"
        android:layout_marginTop="56dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/imageViewRice"
        android:layout_marginEnd="15dp" />


</RelativeLayout>